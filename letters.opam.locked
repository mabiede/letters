  "hxd" {= "0.3.3"}
  "ipaddr" {= "5.6.0"}
  "kdf" {= "1.0.0"}
  "ke" {= "0.6"}
  "logs" {= "0.7.0"}
  "lwt" {= "5.9.0"}
  "macaddr" {= "5.6.0"}
  "mirage-crypto" {= "1.1.0"}
  "mirage-crypto-ec" {= "1.1.0"}
  "mirage-crypto-pk" {= "1.1.0"}
