  "bos" {= "0.2.1"}
  "ca-certs" {= "1.0.0"}
  "cmdliner" {= "1.3.0"}
  "coin" {= "0.1.4"}
  "colombe" {= "0.11.0"}
  "conf-gmp" {= "4"}
  "conf-gmp-powm-sec" {= "3"}
  "conf-pkg-config" {= "3"}
  "containers" {= "3.15"}
